page{
  width: 100%;
  height:100%;
}

.wraper{
  display: flex;
  flex-direction: column;
  width: 100%;
  height:100%;
}
.header{
  position: fixed;
  overflow:scroll;
  width: 100%;
  top: 0;
  z-index: 9999999;
}

.main{
  flex: 1;
  position: relative;
}

.main-scroll{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

.main-list{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-left: 2vw;
  margin-right: 2vw;
}

.title{
  margin-left: 36%;
}
